@model gitHubTest.Models.User;
@using Microsoft.AspNetCore.Http
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>로그인 성공 페이지</title>
</head>
<body>
    <partial name="/Views/DevelopmentRabbit/Contents.cshtml" />
    @if (Context.Session.GetInt32("USER_LOGIN_KEY") == null)
    {
        <button type="submit" class="btn btn-primary">로그인</button>
        <a class="btn btn-warning" href="javascript:void(0);">취소</a>
    }
    else
    {
        <a asp-area="" asp-controller="Account" asp-action="Logout">로그아웃</a>
    }
</body>
</html>
